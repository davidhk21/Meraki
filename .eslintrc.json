{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "arrow-parens": "off",
        "consistent-return": "off",
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"] }],
        "arrow-body-style": "off",
        "object-curly-newline": "off"
    }
}
